        "error",
        "after"
      ],
      "import/extensions": [
        "warn",
        "ignorePackages"
      ],
      "no-console": [
        "off"
      ]
    }
  },
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "express": "^4.17.1",
    "readline-sync": "^1.4.10"
  }
